{
  "description": "GlusterFS volume plugin for Docker",
  "documentation": "https://docs.docker.com/engine/extend/plugins/",
  "entrypoint": [
    "/tini",
    "--",
    "/docker-volume-glusterfs"
  ],
  "network": {
    "type": "host"
  },
  "interface": {
    "socket": "glusterfs.sock",
    "types": [
      "docker.volumedriver/1.0"
    ]
  },
  "linux": {
    "capabilities": [
      "CAP_SYS_ADMIN"
    ],
    "devices": [
      {
        "name": "fuse",
        "path": "/dev/fuse"
      }
    ]
  },
  "propagatedMount": "/var/lib/glusterfs-volume"
}
